{
	"$schema": "../aces.schema.json",
	
	"custom": {
		"conditions": [
			{
				"id": "is-large-number",
				"scriptName": "IsLargeNumber",
				"highlight": true,
				"params": [
					{
						"id": "number",
						"type": "number"
					}
				]
			}
		],
		"actions": [
			{
				"id": "do-alert",
				"scriptName": "Alert",
				"highlight": true
			},
			{
				"id": "create-model",
				"scriptName": "CreateModel",
				"highlight": true,
				"params": [
					{
						"id": "model-path",
						"type": "string"
					}
				]
			},
			{
				"id": "play-animation",
				"scriptName": "PlayAnimation",
				"params": [
					{
						"id": "animation-name",
						"type": "string"
					},
					{
						"id": "loop",
						"type": "boolean"
					}
				]
			},
			{
				"id": "stop-animation",
				"scriptName": "StopAnimation"
			}
		],
		"expressions": [
			{
				"id": "double",
				"expressionName": "Double",
				"highlight": true,
				"returnType": "number",
				"params": [
					{
						"id": "number",
						"type": "number"
					}
				]
			}
		]
	}
}